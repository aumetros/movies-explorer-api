{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "max-classes-per-file": [
      "error",
      { "ignoreExpressions": true, "max": 10 }
  ],
    "no-useless-escape": [0],
    "no-console":"off",
    "consistent-return":"off",
    "func-names":"off",
    "object-shorthand":"off"
  }
}